{
  "name": "no-git",
  "displayName": "No Git",
  "description": "Simple version control without Git",
  "version": "0.0.1",
  "publisher": "Shellomo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shellomo/vscode_ext_nogit"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "snapshot",
    "backup",
    "version control",
    "save",
    "restore",
    "history",
    "undo",
    "git",
    "no-git",
    "simple version control",
    "simple git"
  ],
  "activationEvents": [ "onCommand:no-git.createSnapshot", "onCommand:no-git.restoreSnapshot", "onCommand:no-git.listSnapshots" ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "no-git.createSnapshot",
        "title": "No Git: Create Snapshot"
      },
      {
        "command": "no-git.restoreSnapshot",
        "title": "No Git: Restore Snapshot"
      },
      {
        "command": "no-git.listSnapshots",
        "title": "No Git: List Snapshots"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.x",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.5.0"
  }
}